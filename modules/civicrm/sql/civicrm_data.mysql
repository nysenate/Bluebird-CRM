                              <td>
                                {$line.line_total_inclusive|crmMoney:\'{contribution.currency}\'}
                              </td>
                            {/if}
                            <td>
                              {$line.membership.start_date|crmDate:"Full"}
                            </td>
                            <td>
                              {$line.membership.end_date|crmDate:"Full"}
                            </td>
                          </tr>
                        {/foreach}
