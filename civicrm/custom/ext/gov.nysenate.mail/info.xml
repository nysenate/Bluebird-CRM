<?xml version="1.0"?>
<extension key="gov.nysenate.mail" type="module">
  <file>mail</file>
  <name>NYSS: Mailing Customizations</name>
  <description/>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>NYSS Senate</author>
    <email>brian@lcdservices.biz</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2017-05-23</releaseDate>
  <version>1.2</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/NYSS/Mail</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_NYSS_Mail_Upgrader</upgrader>
</extension>
