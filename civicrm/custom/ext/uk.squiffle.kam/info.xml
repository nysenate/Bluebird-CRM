<?xml version="1.0"?>
<extension key="uk.squiffle.kam" type="module">
  <file>kam</file>
  <name>Keyboard Accessible Menu</name>
  <description>Replaces the default admin menu with an accessible, responsive one using the SmartMenus jQuery menu plugin (smartmenus.org)</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Aidan Saunders</author>
    <email>aidan.saunders@squiffle.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/aydun/uk.squiffle.kam</url>
    <url desc="Documentation">https://github.com/aydun/uk.squiffle.kam</url>
    <url desc="Support">https://github.com/aydun/uk.squiffle.kam</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2019-01-17</releaseDate>
  <version>1.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.8</ver>
  </compatibility>
  <comments>This extension will be ported to core CiviCRM in the near future.</comments>
  <civix>
    <namespace>CRM/Kam</namespace>
  </civix>
</extension>
