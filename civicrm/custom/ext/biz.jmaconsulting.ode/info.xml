<?xml version="1.0"?>
<extension key="biz.jmaconsulting.ode" type="module">
  <downloadUrl>https://github.com/JMAConsulting/biz.jmaconsulting.ode/archive/ode.1.7.zip</downloadUrl>
  <file>ode</file>
  <name>Outbound Domain Enforcement</name>
  <description>This CiviCRM extension is designed to preserve the email reputation of your server and its IP by ensuring that all outbound emailis sent from an address with the same domain as the System-generated Mail Settings From Email Address configured at Administer &gt; Communications &gt; Organization Address and Contact Info.</description>
  <urls>
    <url desc="Support">https://github.com/JMAConsulting/biz.jmaconsulting.ode/issues?state=open</url>
    <url desc="Documentation">https://github.com/JMAConsulting/biz.jmaconsulting.ode/blob/master/README.md</url>
    <url desc="Installation Instructions for 1.8">https://github.com/JMAConsulting/biz.jmaconsulting.ode/blob/master/README.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <license>AGPL 3.0</license>
  <maintainer>
    <author>JMA Consulting</author>
    <email>joe.murray@jmaconsulting.biz</email>
  </maintainer>
  <releaseDate>2020-12-15</releaseDate>
  <version>1.9</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.9</ver>
  </compatibility>
  <comments>Thanks to Brescia University College for sponsoring the development of this module.</comments>
  <civix>
    <namespace>CRM/Ode</namespace>
    <format>22.10.0</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
</extension>
